{
  "name": "@k8skonf/core",
  "version": "1.32.6",
  "author": {
    "name": "konfjs",
    "url": "https://github.com/konfjs"
  },
  "license": "MPL-2.0",
  "homepage": "https://github.com/konfjs/k8skonf",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/konfjs/k8skonf.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/konfjs/k8skonf/issues"
  },
  "keywords": [
    "konfjs",
    "k8skonf",
    "kubernetes",
    "cdk8s",
    "cdk",
    "helm"
  ],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "./*": {
        "import": "./dist/models/*.js",
        "types": "./dist/models/*.d.ts"
      }
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./*": {
      "import": "./src/models/*.ts",
      "types": "./src/models/*.ts"
    }
  },
  "scripts": {
    "lint": "biome lint",
    "build": "rm -rf dist && tsc",
    "prepack": "pnpm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "5.7.2"
  },
  "dependencies": {
    "picocolors": "^1.1.1",
    "yaml": "^2.6.1"
  }
}
