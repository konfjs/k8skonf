{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.10.0",
    "generators": {
      "v3.0": {
        "generatorName": "typescript",
        "glob": "**/v3/*.json",
        "output": "./src/gen",
        "additionalProperties": {
          "supportsES6": true,
          "importFileExtension": ".js"
        },
        "globalProperty": "models,supportingFiles"
      }
    }
  }
}
